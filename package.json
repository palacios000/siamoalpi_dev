    "uikit": "^3.7.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "node-sass": "^6.0.1",
